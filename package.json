{
  "name": "timhortons",
  "version": "0.1.0",
  "description": "Tim Hortons store finder",
  "main": "index.js",
  "repository": "git+https://github.com/kirkeaton/timhortons.git",
  "author": "Kirk Eaton <kirk.eaton@gmail.com>",
  "license": "MIT",
  "keywords": [
    "tim",
    "hortons",
    "store",
    "finder"
  ],
  "scripts": {
    "lint": "./node_modules/xo/cli.js",
    "test": "node ./node_modules/@hapi/lab/bin/lab -l -v test/*.js"
  },
  "xo": {
    "space": true,
    "esnext": false,
    "rules": {
      "unicorn/filename-case": 0,
      "import/no-unassigned-import": 0,
      "no-negated-condition": 0,
      "no-else-return": 0,
      "padded-blocks": 0,
      "quote-props": 0,
      "new-cap": 0,
      "object-shorthand": 0,
      "object-curly-spacing": 0,
      "no-use-extend-native/no-use-extend-native": 0,
      "no-template-curly-in-string": 0,
      "camelcase": 0
    },
    "ignore": [
      "test/*.*"
    ]
  },
  "dependencies": {
    "@hapi/hoek": "^7.1.0",
    "bluebird": "^3.5.5",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@hapi/lab": "^19.1.0",
    "xo": "^0.18.1"
  }
}
